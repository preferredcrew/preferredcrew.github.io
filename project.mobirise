    "fleetcaptain.html": {
      "settings": {
        "title": "Fleet Captain",
        "meta_descr": "Web Site Builder Description",
        "order": 15
      },
      "components": [
        {
          "_inherit": "msg-box8",
          "_styles": {
            ".mbr-section-title, p": {
              "color": "#000"
