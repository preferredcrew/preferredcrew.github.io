      "settings": {
        "title": "2448",
        "meta_descr": "Yacht Stew",
        "order": 14
      },
      "components": [
        {
          "_inherit": "msg-box8",
          "_styles": {
            ".mbr-section-title, p": {
              "color": "#000"
